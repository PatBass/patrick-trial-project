				<Choice id="1" value="prive_non_cadre" label="non-cadre"></Choice>
				<Choice id="2" value="prive_cadre" label="cadre"></Choice>
			</Choices>
		</Data>
		<Data id="6" name="salaire" label="Salaire" type="money" min="0" max="999999999" content="2300"></Data>
