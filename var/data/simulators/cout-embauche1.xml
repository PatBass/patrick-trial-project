				<Choice id="1" value="1" label="brut"></Choice>
				<Choice id="2" value="2" label="net"></Choice>
			</Choices>
		</Data>
		<Data id="8" name="tempsDeTravail" label="Temps de travail" type="choice" default="'temps_plein'">
			<Choices>
				<Choice id="1" value="temps_plein" label="temps plein"></Choice>
