			<Choices>
				<Choice id="1" value="CDI" label="CDI"></Choice>
				<Choice id="2" value="apprenti" label="apprenti-e"></Choice>
			</Choices>
		</Data>
		<Data id="5" name="categorieSalarie" label="Categorie Salarié" type="choice" default="'prive_non_cadre'">
